/dts-v1/;
#include "exynos7580-reserve-2000MB-Ax.dtsi"
#include "exynos7580-j7xelte_common.dtsi"
#include "modem-carmen2-reserve-2000MB.dtsi"
#include "exynos7580-j7xelte_battery_00.dtsi"
#include "exynos7580-j7xelte_chn_cmcc_gpio_00.dtsi"
#include "exynos7580-a7xelte_mst_00.dtsi"

/ {
	model = "Samsung J7XE revision 00 board based on EXYNOS7580";
	model_info-chip = <7580>;
	model_info-platform = "android";
	model_info-subtype = "samsung";
	model_info-hw_rev = <0>;
	model_info-hw_rev_end = <0>;
	compatible = "samsung,exynos7580-universal7580,r01", "samsung,exynos7580";

	pinctrl@139B0000 {
		pmic_irq: pmic-irq {
			samsung,pins = "gpa0-2";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		cod3022_irq: cod3022-irq {
			samsung,pins = "gpa0-4";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		if_irq: if-irq {
			samsung,pins = "gpa0-5";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		ps_als: ps_als {
			samsung,pins = "gpa0-7";
			samsung,pin-function = <0x0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		attn_irq: attn-irq {
			samsung,pins = "gpa1-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		muic_irq: muic-irq {
			samsung,pins = "gpa1-3";
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		bhy_int: bhy-int {
			samsung,pins = "gpa3-0";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		dwmmc2_cd_ext_irq: dwmmc2_cd_ext_irq {
			samsung,pins = "gpa3-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		gpiodetect_irq: gpiodetect-irq {
			samsung,pins = "gpa3-4";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
/*
		certify_hall_irq: certify-hall-irq {
			samsung,pins = "gpa3-5";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		hall_irq: hall_irq {
			samsung,pins = "gpa3-5";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
*/
	};

	pinctrl@139C0000 {
		optical: optical {
			samsung,pins = "gpd3-1", "gpd3-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		sensor_i2c: sensor_i2c {
			samsung,pins = "gpc2-1", "gpc2-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
		};
		main_mic_bias_en:main-mic-bias-en {
			samsung,pins ="gpd2-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
			samsung,pin-val = <0>;
		};
		/* Warm reset information from AP */
		pm_wrsti: pm-wrsti {
			samsung,pins = "gpe1-0";
			samsung,pin-con-pdn = <3>;
		};
		dbmd_reset: dbmd-reset {
			samsung,pins ="gpf1-6";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn =<3>;
			samsung,pin-pud-pdn = <1>;
			samsung,pin-val = <0>;
		};
		dbmd_wakeup: dbmd-wakeup {
			samsung,pins ="gpf1-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn =<3>;
			samsung,pin-pud-pdn = <1>;
			samsung,pin-val = <0>;
		};
	};

	i2c@13840000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
        	samsung,i2c-max-bus-freq = <400000>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c7_bus>;
		
		pn547@2B {
			compatible = "pn547";
			reg = <0x2B>;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpa1>;
			pn547,nfc_pd = <&gpc3 2 0x2>;
			pn547,firm-gpio = <&gpe0 0 0>;
			pn547,irq-gpio = <&gpa1 2 0>;
			clkctrl-reg = <0x1046600C>;
			pn547,nfc_clkreq = <&gpc3 3 0x2>;
			pn547,i2c_1p8 = "vdd_ldo3";
			pinctrl-names = "default";
			pinctrl-0 = <&nfc_n5_firm &nfc_n5_clk_req &nfc_pd &nfc_int>;
		};
	};

	hsi2c@13870000 {
		status = "okay";
		/*samsung,hs-mode;*/
		clock-frequency = <400000>;
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&sensor_i2c>;

		k2hh@1D {
			compatible = "k2hh-i2c";
			reg = <0x1D>;
			interrupt-parent = <&gpa3>;
			interrupts = <0 0 0>;
			k2hh,irq_gpio = <&gpa3 0 0>;
			k2hh,vio-supply = <&ldo3_reg>;
			k2hh,axis_map_x = <0>;
			k2hh,axis_map_y = <1>;
			k2hh,axis_map_z = <2>;
			k2hh,negate_x = <0>;
			k2hh,negate_y = <0>;
			k2hh,negate_z = <1>;
			k2hh,poll_interval = <100>;
			k2hh,min_interval = <2>;
		};
	};
	
	hsi2c@104c0000 {
		audio_codec_cod3022x: cod3022x@04 {
			compatible = "codec,cod3022x";
			reg = <0x04>;
			ten-bit-address;
			vdd1-supply = <&ldo22_reg>;
			vdd2-supply = <&ldo23_reg>;
			pinctrl-names = "default";
			pinctrl-0 = <&cod3022_irq>;
			interrupt-parent = <&gpa0>;
			gpios = <&gpa0 4 0xf>;
			/* Mic Bias: 0->2.1v, 1->2.5v, 2->2.6v, 3->3.0v */
			mic-bias1-voltage = <1>;
			mic-bias2-voltage = <1>;
			use-btn-adc-mode;
			io-channels = <&exynos_adc 2>;
			io-channel-names = "adc-jack";
			#io-channel-cells = <1>;
			io-channel-ranges;
			but-zones-list = <&but_zones 226 0 245>, /* send/end */
					<&but_zones 582 246 315>, /* google */
					<&but_zones 115 316 520>, /* vol up */
					<&but_zones 114 521 900>; /* vol down */
			mic-adc-range = <1380>;
			btn-release-value = <1000>;
			mic-det-delay = <40>;
		};
	};

	i2c@33 {
		compatible = "i2c-gpio";
		gpios = <&gpd3 2 0	/* sda */
			&gpd3 1 0>;	/* scl */

		pinctrl-names = "default";
		pinctrl-0 = <&optical>;
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		cm36686@60 {
			compatible = "cm36686";
			reg = <0x60>;
			pinctrl-names = "dafault";
			pinctrl-0 = <&ps_als>;
			interrupt-parent = <&gpa0>;
			interrupts = <7 0 0>;
			cm36686,vdd-supply = <&ldo37_reg>;
			cm36686,vled-supply = <&ldo37_reg>;
			cm36686,irq_gpio = <&gpa0 7 0x00>;
			cm36686,default_hi_thd = <11>;
			cm36686,default_low_thd = <8>;
			cm36686,cancel_hi_thd = <10>;
			cm36686,cancel_low_thd = <7>;
			cm36686,default_trim = <0x0003>;
			cm36686,vdd_always_on = <0>;
			cm36686,vled_same_vdd = <1>;
		};
	};

	pinctrl@139B0000 {
		bt_enable: bt_enable {
			 samsung,pins = "gpa1-6";
			 samsung,pin-function = <1>;
			 samsung,pin-pud = <0>;
		};
	};

	pinctrl@139C0000 {
		uart0_rts: uart0-rts {
			samsung,pins = "gpb0-7";
			samsung,pin-function = <2>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <1>;
		};

		uart0_cts: uart0-cts {
			samsung,pins = "gpb0-6";
			samsung,pin-function = <2>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <1>;
		};

		uart0_tx: uart0-tx {
			samsung,pins = "gpb0-5";
			samsung,pin-function = <2>;
			samsung,pin-pud = <3>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <3>;
		};

		uart0_rx: uart0-rx {
			samsung,pins = "gpb0-4";
			samsung,pin-function = <2>;
			samsung,pin-pud = <3>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <3>;
		};
	};

	mif_pdata {
		pinctrl-names = "default";
		pinctrl-0 = <&cp_rst_n &ds_det>;

		/* To do: will be remove, for system revision */
		gpios = <&gpd0 0 0x0>,
		       <&gpd0 1 0x0>,
		       <&gpd0 2 0x0>,
		       <&gpd0 3 0x0>;

		modem_ctrl,gpio_cp_rst_n = <&gpe0 6 0x1>;
		mif,gpio_ds_det = <&gpd0 4 0x0>;

		mif,ap_clk_table = <
			/* khz */
			1600000
			1500000
			1400000
			1300000
			1200000
			1100000
			1000000
			900000
			800000
			700000
			600000
			500000
			400000
			300000
			>;

		mif,mif_clk_table = <
			/* khz */
			825000
			728000
			667000
			559000
			416000
			338000
			273000
			200000
			>;
	};

	pinctrl@139C0000 {
		ds_det: ds_det {
			samsung,pins = "gpd0-4";
			samsung,pin-function = <0>;	/* INPUT */
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <3>;	/* PREV STATE */
			samsung,pin-pud-pdn = <0>;
		};

		cp_rst_n: cp_rst_n {
			samsung,pins = "gpe0-6";
			samsung,pin-function = <1>;	/* OUTPUT */
			samsung,pin-pud = <0>;		/* PU & PD DISABLE */
			samsung,pin-con-pdn = <1>;	/* OUTPUT 1 */
			samsung,pin-pud-pdn = <0>;	/* PU & PD DISABLE */
		};

		leden_gpio: leden_gpio {
			samsung,pins = "gpd2-3";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-val = <1>;
		};
	};
};
